{
  "manifest_version": 2,
  "name": "Media Source Extractor",
  "version": "1.3",
  "description": "Extracts media source into a clean iframe",
  "content_scripts": [
    {
      "matches": ["*://*.to/serialy/*"],
      "js": ["main.js"],
      "css": ["main.css"],
      "run_at": "document_start"
    }
  ]
}